{
  "apiVersion":"1",
  "version":"@project.version@",
  "provider": {
    "name":"Cumulocity GmbH"
  },
  "isolation":"MULTI_TENANT",
  "requiredRoles": [
  	"ROLE_INVENTORY_READ",
    "ROLE_INVENTORY_ADMIN",
    "ROLE_MEASUREMENT_READ",
    "ROLE_MEASUREMENT_ADMIN",
    "ROLE_EVENT_ADMIN",
    "ROLE_EVENT_READ"
  ],
  "roles":[
  ],
  "livenessProbe":{
    "httpGet":{
      "path": "/health",
      "port": 80

    },
    "initialDelaySeconds": 200,
    "periodSeconds": 10
  },
  "readinessProbe":{
    "httpGet":{
      "path": "/health",
      "port": 80

    },
    "initialDelaySeconds": 220,
    "periodSeconds": 10
  }
}
